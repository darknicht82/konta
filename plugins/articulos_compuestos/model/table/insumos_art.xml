<tabla>
    <columna>
        <nombre>idinsumo</nombre>
        <tipo>serial</tipo>
        <nulo>NO</nulo>
        <defecto>nextval('insumo_id_seq'::regclass)</defecto>
    </columna>
    <columna>
        <nombre>idempresa</nombre>
        <tipo>integer</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>idarticulocomp</nombre>
        <tipo>integer</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>idarticulo</nombre>
        <tipo>integer</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>cantidad</nombre>
        <tipo>decimal(16, 6)</tipo>
        <nulo>NO</nulo>
        <defecto>0.000000</defecto>
    </columna>
    <columna>
        <nombre>fec_creacion</nombre>
        <tipo>date</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>nick_creacion</nombre>
        <tipo>character varying(25)</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>fec_modificacion</nombre>
        <tipo>date</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>nick_modificacion</nombre>
        <tipo>character varying(25)</tipo>
        <nulo>YES</nulo>
    </columna>
    <restriccion>
        <nombre>insumos_art_pkey</nombre>
        <consulta>PRIMARY KEY (idinsumo)</consulta>
    </restriccion>
    <restriccion>
        <nombre>fk_empresa_insumos_art</nombre>
        <consulta>FOREIGN KEY (idempresa) REFERENCES empresa (idempresa) ON DELETE CASCADE ON UPDATE CASCADE</consulta>
    </restriccion>
    <restriccion>
        <nombre>fk_articuloscomp_insumos_art</nombre>
        <consulta>FOREIGN KEY (idarticulocomp) REFERENCES articulos (idarticulo) ON DELETE RESTRICT ON UPDATE CASCADE</consulta>
    </restriccion>
    <restriccion>
        <nombre>fk_articulos_insumos_art</nombre>
        <consulta>FOREIGN KEY (idarticulo) REFERENCES articulos (idarticulo) ON DELETE RESTRICT ON UPDATE CASCADE</consulta>
    </restriccion>
</tabla>