    </columna>
    <columna>
        <nombre>codprincipal</nombre>
        <tipo>character varying(100)</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>descripcion</nombre>
        <tipo>character varying(250)</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>cantidad</nombre>
        <tipo>decimal(16, 6)</tipo>
        <nulo>NO</nulo>
        <defecto>0</defecto>
    </columna>
    <columna>
        <nombre>nueva_cantidad</nombre>
        <tipo>decimal(16, 6)</tipo>
        <nulo>NO</nulo>
        <defecto>0</defecto>
