    <restriccion>
        <nombre>fk_regularizaciones_lineasregularizaciones</nombre>
        <consulta>FOREIGN KEY (idregularizacion) REFERENCES regularizaciones (idregularizacion) ON DELETE CASCADE ON UPDATE CASCADE</consulta>
    </restriccion>
    <restriccion>
        <nombre>fk_articulos_lineasregularizaciones</nombre>
        <consulta>FOREIGN KEY (idarticulo) REFERENCES articulos (idarticulo) ON DELETE RESTRICT ON UPDATE CASCADE</consulta>
    </restriccion>
