<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : access.xml
    Author     : Jonathan Guamba
    Description:
        Estructura de la tabla access.
-->
<tabla>
    <columna>
        <nombre>nick</nombre>
        <tipo>character varying(25)</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>page</nombre>
        <tipo>character varying(30)</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>allow_delete</nombre>
        <tipo>boolean</tipo>
        <nulo>NO</nulo>
        <defecto>true</defecto>
    </columna>
    <columna>
        <nombre>allow_modify</nombre>
        <tipo>boolean</tipo>
        <nulo>NO</nulo>
        <defecto>true</defecto>
    </columna>
    <columna>
        <nombre>allow_download</nombre>
        <tipo>boolean</tipo>
        <nulo>NO</nulo>
        <defecto>true</defecto>
    </columna>
    <restriccion>
        <nombre>access_pkey</nombre>
        <consulta>PRIMARY KEY (nick, page)</consulta>
    </restriccion>
    <restriccion>
        <nombre>access_user2</nombre>
        <consulta>FOREIGN KEY (nick) REFERENCES users (nick) ON DELETE CASCADE ON UPDATE CASCADE</consulta>
    </restriccion>
    <restriccion>
        <nombre>access_page2</nombre>
        <consulta>FOREIGN KEY (page) REFERENCES pages (name) ON DELETE CASCADE ON UPDATE CASCADE</consulta>
    </restriccion>
</tabla>