    Description:
        Estructura de la tabla tiposretenciones.
-->
<tabla>
    <columna>
        <nombre>idtiporetencion</nombre>
        <tipo>serial</tipo>
        <nulo>NO</nulo>
        <defecto>nextval('tiposretenciones_id_seq'::regclass)</defecto>
    </columna>
    <columna>
        <nombre>idempresa</nombre>
        <tipo>integer</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>codigobase</nombre>
        <tipo>character varying(4)</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>codigo</nombre>
        <tipo>character varying(4)</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>especie</nombre>
        <tipo>character varying(10)</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>nombre</nombre>
        <tipo>character varying(150)</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>porcentaje</nombre>
        <tipo>decimal(16, 6)</tipo>
        <nulo>NO</nulo>
        <defecto>0</defecto>
    </columna>
    <columna>
        <nombre>esventa</nombre>
        <tipo>boolean</tipo>
        <nulo>NO</nulo>
        <defecto>true</defecto>
    </columna>
    <columna>
        <nombre>escompra</nombre>
        <tipo>boolean</tipo>
        <nulo>NO</nulo>
        <defecto>true</defecto>
    </columna>
    <columna>
        <nombre>fec_creacion</nombre>
        <tipo>date</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>nick_creacion</nombre>
        <tipo>character varying(25)</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>fec_modificacion</nombre>
        <tipo>date</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>nick_modificacion</nombre>
        <tipo>character varying(25)</tipo>
        <nulo>YES</nulo>
    </columna>
    <restriccion>
        <nombre>tiposretenciones_pkey</nombre>
        <consulta>PRIMARY KEY (idtiporetencion)</consulta>
    </restriccion>
    <restriccion>
        <nombre>fk_empresa_tiposretenciones</nombre>
        <consulta>FOREIGN KEY (idempresa) REFERENCES empresa (idempresa) ON DELETE CASCADE ON UPDATE CASCADE</consulta>
    </restriccion>
</tabla>