<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : logs.xml
    Author     : Carlos Garcia Gomez
    Description:
        Estructura de la tabla logs.
-->
<tabla>
    <columna>
        <nombre>alerta</nombre>
        <tipo>boolean</tipo>
    </columna>
    <columna>
        <nombre>controlador</nombre>
        <tipo>character varying(100)</tipo>
    </columna>
    <columna>
        <nombre>detalle</nombre>
        <tipo>text</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>fecha</nombre>
        <tipo>timestamp</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>id</nombre>
        <tipo>serial</tipo>
        <nulo>NO</nulo>
        <defecto>nextval('logs_id_seq'::regclass)</defecto>
    </columna>
    <columna>
        <nombre>ip</nombre>
        <tipo>character varying(40)</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>tipo</nombre>
        <tipo>character varying(50)</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>usuario</nombre>
        <tipo>character varying(25)</tipo>
    </columna>
    <restriccion>
        <nombre>logs_pkey</nombre>
        <consulta>PRIMARY KEY (id)</consulta>
    </restriccion>
</tabla>
