<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : marcas.xml
    Description:
        Estructura de la tabla marcas.
-->
<tabla>
    <columna>
        <nombre>idmarca</nombre>
        <tipo>serial</tipo>
        <nulo>NO</nulo>
        <defecto>nextval('marcas_id_seq'::regclass)</defecto>
    </columna>
    <columna>
        <nombre>idempresa</nombre>
        <tipo>integer</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>nombre</nombre>
        <tipo>character varying(250)</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>imagen</nombre>
        <tipo>character varying(250)</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>idpadre</nombre>
        <tipo>integer</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>fec_creacion</nombre>
        <tipo>date</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>nick_creacion</nombre>
        <tipo>character varying(25)</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>fec_modificacion</nombre>
        <tipo>date</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>nick_modificacion</nombre>
        <tipo>character varying(25)</tipo>
        <nulo>YES</nulo>
    </columna>
    <restriccion>
        <nombre>marcas_pkey</nombre>
        <consulta>PRIMARY KEY (idmarca)</consulta>
    </restriccion>
    <restriccion>
        <nombre>fk_empresa_marcas</nombre>
        <consulta>FOREIGN KEY (idempresa) REFERENCES empresa (idempresa) ON DELETE CASCADE ON UPDATE CASCADE</consulta>
    </restriccion>
</tabla>