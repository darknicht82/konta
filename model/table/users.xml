<tabla>
    <columna>
        <nombre>nick</nombre>
        <tipo>character varying(25)</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>password</nombre>
        <tipo>character varying(100)</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>idempresa</nombre>
        <tipo>integer</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>nick_supervisor</nombre>
        <tipo>character varying(25)</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>log_key</nombre>
        <tipo>character varying(100)</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>apellidos</nombre>
        <tipo>character varying(100)</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>nombres</nombre>
        <tipo>character varying(100)</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>admin</nombre>
        <tipo>boolean</tipo>
        <nulo>NO</nulo>
        <defecto>false</defecto>
    </columna>
    <columna>
        <nombre>supervisor</nombre>
        <tipo>boolean</tipo>
        <nulo>NO</nulo>
        <defecto>false</defecto>
    </columna>
    <columna>
        <nombre>activo</nombre>
        <tipo>boolean</tipo>
        <nulo>NO</nulo>
        <defecto>true</defecto>
    </columna>   
    <columna>
        <nombre>last_login</nombre>
        <tipo>date</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>last_login_time</nombre>
        <tipo>time</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>last_ip</nombre>
        <tipo>character varying(40)</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>last_browser</nombre>
        <tipo>character varying(255)</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>ppage</nombre>
        <tipo>character varying(100)</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>thema</nombre>
        <tipo>character varying(100)</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>idcliente</nombre>
        <tipo>integer</tipo>
        <nulo>YES</nulo>
        <defecto>NULL</defecto>
    </columna>
    <columna>
        <nombre>fec_creacion</nombre>
        <tipo>date</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>nick_creacion</nombre>
        <tipo>character varying(25)</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>fec_modificacion</nombre>
        <tipo>date</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>nick_modificacion</nombre>
        <tipo>character varying(25)</tipo>
        <nulo>YES</nulo>
    </columna>
    <restriccion>
        <nombre>users_pkey</nombre>
        <consulta>PRIMARY KEY (nick)</consulta>
    </restriccion>
    <restriccion>
        <nombre>fk_users_pages</nombre>
        <consulta>FOREIGN KEY (ppage) REFERENCES pages (name) ON DELETE SET NULL ON UPDATE CASCADE</consulta>
    </restriccion>
    <restriccion>
        <nombre>fk_users_empresa</nombre>
        <consulta>FOREIGN KEY (idempresa) REFERENCES empresa (idempresa) ON DELETE RESTRICT ON UPDATE CASCADE</consulta>
    </restriccion>
    <restriccion>
        <nombre>fk_users_clientes</nombre>
        <consulta>FOREIGN KEY (idcliente) REFERENCES clientes (idcliente) ON DELETE RESTRICT ON UPDATE CASCADE</consulta>
    </restriccion>
</tabla>