<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : movimientos.xml
    Author     : Jonathan Guamba
    Description:
        Estructura de la tabla movimientos.
-->
<tabla>
    <columna>
        <nombre>idmovimiento</nombre>
        <tipo>serial</tipo>
        <nulo>NO</nulo>
        <defecto>nextval('movimientos_id_seq'::regclass)</defecto>
    </columna>
    <columna>
        <nombre>idempresa</nombre>
        <tipo>integer</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>idestablecimiento</nombre>
        <tipo>integer</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>tipo</nombre>
        <tipo>character varying(10)</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>numero</nombre>
        <tipo>integer</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>fec_emision</nombre>
        <tipo>date</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>hora_emision</nombre>
        <tipo>time</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>total</nombre>
        <tipo>decimal(16, 6)</tipo>
        <nulo>NO</nulo>
        <defecto>0</defecto>
    </columna>
    <columna>
        <nombre>observaciones</nombre>
        <tipo>character varying(300)</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>idejercicio</nombre>
        <tipo>integer</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>fec_creacion</nombre>
        <tipo>date</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>nick_creacion</nombre>
        <tipo>character varying(25)</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>fec_modificacion</nombre>
        <tipo>date</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>nick_modificacion</nombre>
        <tipo>character varying(25)</tipo>
        <nulo>YES</nulo>
    </columna>
    <restriccion>
        <nombre>movimientos_pkey</nombre>
        <consulta>PRIMARY KEY (idmovimiento)</consulta>
    </restriccion>
    <restriccion>
        <nombre>fk_empresa_movimientos</nombre>
        <consulta>FOREIGN KEY (idempresa) REFERENCES empresa (idempresa) ON DELETE CASCADE ON UPDATE CASCADE</consulta>
    </restriccion>
    <restriccion>
        <nombre>fk_establecimiento_movimientos</nombre>
        <consulta>FOREIGN KEY (idestablecimiento) REFERENCES establecimiento (idestablecimiento) ON DELETE RESTRICT ON UPDATE CASCADE</consulta>
    </restriccion>
</tabla>