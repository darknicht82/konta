<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : stocks.xml
    Description:
        Estructura de la tabla stocks.
-->
<tabla>
    <columna>
        <nombre>idstock</nombre>
        <tipo>serial</tipo>
        <nulo>NO</nulo>
        <defecto>nextval('stocks_id_seq'::regclass)</defecto>
    </columna>
    <columna>
        <nombre>idempresa</nombre>
        <tipo>integer</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>idestablecimiento</nombre>
        <tipo>integer</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>idarticulo</nombre>
        <tipo>integer</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>stock</nombre>
        <tipo>decimal(16, 6)</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>ubicacion</nombre>
        <tipo>character varying(150)</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>fec_creacion</nombre>
        <tipo>date</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>nick_creacion</nombre>
        <tipo>character varying(25)</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>fec_modificacion</nombre>
        <tipo>date</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>nick_modificacion</nombre>
        <tipo>character varying(25)</tipo>
        <nulo>YES</nulo>
    </columna>
    <restriccion>
        <nombre>stocks_pkey</nombre>
        <consulta>PRIMARY KEY (idstock)</consulta>
    </restriccion>
    <restriccion>
        <nombre>fk_empresa_stocks</nombre>
        <consulta>FOREIGN KEY (idempresa) REFERENCES empresa (idempresa) ON DELETE CASCADE ON UPDATE CASCADE</consulta>
    </restriccion>
    <restriccion>
        <nombre>fk_establecimiento_stocks</nombre>
        <consulta>FOREIGN KEY (idestablecimiento) REFERENCES establecimiento (idestablecimiento) ON DELETE CASCADE ON UPDATE CASCADE</consulta>
    </restriccion>
    <restriccion>
        <nombre>fk_articulos_stocks</nombre>
        <consulta>FOREIGN KEY (idarticulo) REFERENCES articulos (idarticulo) ON DELETE CASCADE ON UPDATE CASCADE</consulta>
    </restriccion>
</tabla>