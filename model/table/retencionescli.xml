<tabla>
    <columna>
        <nombre>idretencion</nombre>
        <tipo>serial</tipo>
        <nulo>NO</nulo>
        <defecto>nextval('retencionescli_id_seq'::regclass)</defecto>
    </columna>
    <columna>
        <nombre>idempresa</nombre>
        <tipo>integer</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>idcliente</nombre>
        <tipo>integer</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>fec_emision</nombre>
        <tipo>date</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>fec_registro</nombre>
        <tipo>date</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>numero_documento</nombre>
        <tipo>character varying(17)</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>nro_autorizacion</nombre>
        <tipo>character varying(49)</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>fec_autorizacion</nombre>
        <tipo>date</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>hor_autorizacion</nombre>
        <tipo>time</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>total</nombre>
        <tipo>decimal(16, 6)</tipo>
        <nulo>NO</nulo>
        <defecto>0</defecto>
    </columna>
    <columna>
        <nombre>idejercicio</nombre>
        <tipo>integer</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>fec_creacion</nombre>
        <tipo>date</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>nick_creacion</nombre>
        <tipo>character varying(25)</tipo>
        <nulo>NO</nulo>
    </columna>
    <columna>
        <nombre>fec_modificacion</nombre>
        <tipo>date</tipo>
        <nulo>YES</nulo>
    </columna>
    <columna>
        <nombre>nick_modificacion</nombre>
        <tipo>character varying(25)</tipo>
        <nulo>YES</nulo>
    </columna>
    <restriccion>
        <nombre>retencionescli_pkey</nombre>
        <consulta>PRIMARY KEY (idretencion)</consulta>
    </restriccion>
    <restriccion>
        <nombre>fk_empresa_retencionescli</nombre>
        <consulta>FOREIGN KEY (idempresa) REFERENCES empresa (idempresa) ON DELETE CASCADE ON UPDATE CASCADE</consulta>
    </restriccion>
    <restriccion>
        <nombre>fk_clientes_retencionescli</nombre>
        <consulta>FOREIGN KEY (idcliente) REFERENCES clientes (idcliente) ON DELETE RESTRICT ON UPDATE CASCADE</consulta>
    </restriccion>
</tabla>